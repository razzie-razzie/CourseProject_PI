@model IEnumerable<CourseProject.DishesType>

@{
    ViewBag.Title = "Список типов блюд";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 class="page_header">Список типов блюд</h2>
<br />
<table class="my_table">
    <tr class=".my_table .header-line">
        <th>
            @Html.DisplayName("Название типа")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
            <td>
                @Html.ActionLink("Редактировать", "Edit", new { id = item.ID }) |
                @Html.ActionLink("Удалить", "Delete", new { id = item.ID })
            </td>
        </tr>
    }

</table>

<div>
    @Html.ActionLink("Назад", "Index", "Home")
</div>
